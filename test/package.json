{
  "private": true,
  "name": "@typia/test",
  "version": "0.1.0",
  "description": "Test program for typia",
  "main": "bin/index.js",
  "tsp": {
    "tscOptions": {
      "parseAllJsDoc": true
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "rimraf bin-cjs && tsc",
    "build:esm": "rimraf bin-esm && tsc -p ./tsconfig.esm.json",
    "prepare": "ts-patch install",
    "prettier": "prettier ./src/**/*.ts --write",
    "setup": "node script/setup.js",
    "start": "run-p start:*",
    "start:cjs": "node bin-cjs",
    "start:esm": "node bin-esm",
    "template": "ts-node --project script/tsconfig.json script/template.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/typia"
  },
  "keywords": [
    "typia",
    "test"
  ],
  "author": "Jeongho Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/typia/issues"
  },
  "homepage": "https://github.com/samchon/typia#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cli": "^0.11.25",
    "@types/node": "^20.9.4",
    "@types/uuid": "^9.0.7",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "protobufjs": "^7.2.5",
    "randexp": "^0.5.3",
    "source-map-support": "^0.5.21",
    "suppress-warnings": "^1.0.2",
    "tstl": "^3.0.0",
    "uuid": "^9.0.1",
    "typia": "../typia-6.2.2.tgz"
  }
}
