    "@types/cli": "^0.11.25",
    "@types/node": "^20.9.4",
    "@types/uuid": "^9.0.7",
    "chalk": "^4.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.2"
  },
