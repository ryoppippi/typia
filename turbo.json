{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prepare": {
      "dependsOn": [
        "^prepare"
      ]
    },
    "build": {
      "dependsOn": [
        "^prepare",
        "^build"
      ],
      "outputs": [
        "bin/**",
        "lib/**",
        ".next/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
