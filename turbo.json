{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prepare": {
      "dependsOn": [
        "^prepare"
      ]
    },
    "build": {
      "dependsOn": [
        "^prepare",
        "^build"
      ],
      "outputs": [
        "bin/**",
        "lib/**",
        ".next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^prepare",
        "^build",
        "^test"
      ]
    },
    "test:bun": {
      "dependsOn": [
        "^prepare",
        "^build",
        "^test:bun"
      ]
    },
    "start": {
      "dependsOn": [
        "^prepare",
        "^build",
        "^start"
      ]
    },
    "template": {
      "dependsOn": [
        "^prepare",
        "^template",
        "^build"
      ]
    }
  }
}
